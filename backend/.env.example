# AI Food Tracking - Backend

## Environment Variables
# Copy this file to .env and update values

# Application
APP_NAME="AI Food Tracking"
APP_VERSION="1.0.0"
DEBUG=true
ENVIRONMENT=development

# API
API_V1_PREFIX=/api/v1
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/food_tracking
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# JWT Authentication
SECRET_KEY=your-super-secret-key-change-in-production-use-openssl-rand-hex-32
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# AI/ML (Optional - for food recognition)
OPENAI_API_KEY=your-openai-api-key

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379/0

# Logging
LOG_LEVEL=INFO
